<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:ignore="MissingDefaultResource">

    <View
        android:id="@+id/iv_v_round_translucent"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@drawable/guide_common_round_translucent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="h,1:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/iv_v_round"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="@dimen/dp_3"
        android:background="@drawable/guide_common_round"
        app:layout_constraintBottom_toBottomOf="@id/iv_v_round_translucent"
        app:layout_constraintEnd_toEndOf="@id/iv_v_round_translucent"
        app:layout_constraintStart_toStartOf="@id/iv_v_round_translucent"
        app:layout_constraintTop_toTopOf="@id/iv_v_round_translucent" />

</androidx.constraintlayout.widget.ConstraintLayout>